{% assets ['js_cookie'] %}

{% macro renderLink(link) %}
    <li class="{{ link.icon ? ' tulia-toolbar-item-has-icon' : '' }}{{ link.hasChildren() ? ' tulia-toolbar-item-has-children' : '' }}">
        <a href="{{ link.href }}" title="{{ link.label }}" {{ _self.render_attributes(link.attributes) }}>
            {% if link.icon %}
                <span class="tulia-toolbar-item-icon">
                    {{ link.icon|raw }}
                </span>
            {% endif %}
            <span class="tulia-toolbar-item-label">
                {{ link.label }}
            </span>
        </a>
        {% if link.hasChildren() %}
            <ul>
                {% for child in link.children %}
                    {{ _self.renderLink(child) }}
                {% endfor %}
            </ul>
        {% endif %}
    </li>
{% endmacro %}

{% macro render_attributes(attributes) %}
    {% for name, value in attributes %}
        {{ name }}="{{ value }}"
    {% endfor %}
{% endmacro %}

