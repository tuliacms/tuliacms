{
    "name": "tuliacms/tuliacms",
    "version": "0.0.1",
    "type": "project",
    "license": "MIT",
    "require": {
        "tuliacms/cms": "@dev"
    },
    "------------------": {
        "symfony/cache": "~5.0",
        "symfony/http-kernel": "~5.0",
        "symfony/stopwatch": "~5.0",
        "symfony/console": "~5.0",
        "gregwar/captcha": "~1.1"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "~9.2",
        "phpunit/phpunit": "~8.5",
        "mockery/mockery": "~1.3",
        "phpspec/phpspec": "~7.0"
    },
    "-------------------": {
        "phpstan/phpstan": "~0.11",
        "fzaninotto/faker": "~1.8"
    },
    "repositories": [
        { "type": "path", "url": "../packages/cms", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/command-bus", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/assetter", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/datatable", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/dependency-injection", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/form-builder", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/framework", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/hooking", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/image", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/routing", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/shortcode", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/templating", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/theme", "options": {"symlink": true} },
        { "type": "path", "url": "../packages/widget", "options": {"symlink": true} }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Tulia\\Theme\\": "extension/theme",
            "Tulia\\Module\\": "extension/module"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
