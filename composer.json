{
    "name": "symfony/website-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A skeleton to start a new Symfony website",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "*",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer-pack": "*",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-pack": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "thunderer/shortcode": "^0.7.4",
        "intervention/image": "~2.5",
        "jasongrimes/paginator": "~1.0",
        "ramsey/uuid": "~3.8",
        "requtize/assetter": "dev-master",
        "psr/container": "~1.0",
        "ocramius/generated-hydrator": "~3.0",
        "swiftmailer/swiftmailer": "~6.2"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "phpcompatibility/php-compatibility": "~9.2",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~8.5",
        "mockery/mockery": "~1.3",
        "phpspec/phpspec": "~7.0",
        "phpstan/phpstan": "~0.11",
        "symfony/http-foundation": "~5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tulia\\Bundle\\": "src/Bundle",
            "Tulia\\Component\\CommandBus\\": "src/Component/command-bus/src",
            "Tulia\\Component\\Datatable\\": "src/Component/datatable/src",
            "Tulia\\Component\\DependencyInjection\\": "src/Component/dependency-injection/src",
            "Tulia\\Component\\FormBuilder\\": "src/Component/form-builder/src",
            "Tulia\\Component\\Hooking\\": "src/Component/hooking/src",
            "Tulia\\Component\\Image\\": "src/Component/image/src",
            "Tulia\\Component\\Routing\\": "src/Component/routing/src",
            "Tulia\\Component\\Shortcode\\": "src/Component/shortcode/src",
            "Tulia\\Component\\Templating\\": "src/Component/templating/src",
            "Tulia\\Component\\Theme\\": "src/Component/theme/src",
            "Tulia\\Component\\Widget\\": "src/Component/widget/src",
            "Tulia\\Cms\\Shared\\": "src/Shared",
            "Tulia\\Cms\\": "src/Cms"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
