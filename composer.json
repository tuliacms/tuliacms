{
    "type": "project",
    "name": "tuliacms/tuliacms",
    "license": "MIT",
    "version": "0.0.1-alpha",
    "require": {
        "php": ">=8.1",
        "tuliacms/cms": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tulia\\Theme\\": "extension/theme",
            "Tulia\\Module\\": "extension/module",
            "Tulia\\Bundle\\": "core/src/Bundle",
            "Tulia\\Component\\Datatable\\": "core/src/Component/datatable/src",
            "Tulia\\Component\\Image\\": "core/src/Component/image/src",
            "Tulia\\Component\\Importer\\": "core/src/Component/importer/src",
            "Tulia\\Component\\Routing\\": "core/src/Component/routing/src",
            "Tulia\\Component\\Shortcode\\": "core/src/Component/shortcode/src",
            "Tulia\\Component\\Templating\\": "core/src/Component/templating/src",
            "Tulia\\Component\\Theme\\": "core/src/Component/theme/src",
            "Tulia\\Cms\\Migrations\\": "core/src/Migrations",
            "Tulia\\Cms\\Shared\\": "core/src/Shared",
            "Tulia\\Cms\\": "core/src/Cms"
        }
    },
    "require-dev": {
        "symfony/web-profiler-bundle": "5.4.*",
        "symfony/debug-bundle": "5.4.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
